{
  "name": "gs-cypress-automation-framework",
  "version": "0.0.2",
  "description": "Cypress framework for the new Equity Admin Portal",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/globalshares/gscypressautomationframework"
  },
  "author": "Global Shares",
  "license": "ISC",
  "engines": {
    "node": ">=12.22.0"
  },
  "scripts": {
    "clean": "rm -rf ./node_modules/",
    " --- CYPRESS DEV SCRIPTS ---- ": "",
    "cy:open": "cypress open",
    "cy:run": "cypress run --browser chrome",
    " --- CYPRESS PIPELINE SCRIPTS WITHOUT FILE RESULTS ---- ": "",
    "cy:run:chrome": "cypress run --browser chrome --spec 'cypress/integration/**/*.spec.js'",
    "cy:run:firefox": "cypress run --browser firefox --spec 'cypress/integration/**/*.spec.js'",
    "cy:run:edge": "cypress run --browser edge",
    "cy:run:dashboard": "cypress run --record --key a0b2d38f-1ea0-4639-8bb7-c74bdb76591e --tag 'staging,dashboard'",
    "cy:run:tag:chrome": "cypress run --env grepTags=@smoke --browser chrome",
    "cy:run:tag:firefox": "cypress run --env grepTags=@smoke --browser chrome",
    "cy:list:spec:by:names": "find-cypress-specs --names",
    "cy:list:spec:by:tags": "find-cypress-specs --tags",
    " --- Formatting: ESLint and Prettier + Typescript compiler to support JS code ---- ": "",
    "eslint": "eslint --report-unused-disable-directives --max-warnings=0 -c .eslintrc.json .",
    "eslint:fix": "yarn eslint --fix",
    "lint": "yarn eslint && yarn prettier:check",
    "lint:fix": "yarn ts:clear && yarn eslint:fix && yarn prettier:fix",
    "prettier:cli": "prettier \"**/**.{ts,js,tsx}\"",
    "prettier:check": "yarn prettier:cli -l",
    "prettier:fix": "yarn prettier:cli --write --single-quote \"**/**.{ts,js,tsx}\"",
    "ts:clear": "rm -rf dist",
    "compile": "tsc && rm -rf dist"
  },
  "keywords": [
    "tests",
    "js",
    "cypress"
  ],
  "devDependencies": {
    "cypress": "9.4.1",
    "cypress-plugin-snapshots": "1.4.4",
    "cypress-xpath": "1.6.2",
    "cypress-iframe": "1.0.1",
    "@cypress/skip-test": "2.6.1",
    "cypress-wait-until": "1.7.2",
    "@4tw/cypress-drag-drop": "2.1.0",
    "cypress-grep": "2.13.1",
    "find-cypress-specs": "1.10.0",
    "mocha": "9.1.4",
    "eslint": "8.7.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-eslint-plugin": "4.1.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-mocha": "10.0.3",
    "eslint-plugin-prettier": "4.0.0",
    "@babel/eslint-parser": "7.16.5",
    "@babel/core": "7.16.7",
    "prettier": "2.5.1",
    "typescript": "4.5.4",
    "tsc-silent": "1.2.1",
    "dayjs": "1.10.7",
    "cypress-multi-reporters": "1.5.0",
    "mocha-junit-reporter": "2.0.2",
    "junit-report-merger": "3.0.5",
    "cypress-mochawesome-reporter": "2.3.0"
  }
}
